{
    "name": "hannes/iwheb-auth",
    "description": "iWheb Authentication Service with Webling integration",
    "license": "proprietary",
    "require": {
        "php": ">=7.2",
        "ext-pdo": "*",
        "ext-sqlite3": "*",
        "ext-sodium": "*",
        "ext-json": "*",
        "ext-curl": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "iwhebAPI\\UserAuth\\": "src/UserAuth/",
            "iwhebAPI\\SessionManagement\\": "src/SessionManagement/"
        },
        "files": [
            "src/SessionManagement/logging.php"
        ],
        "classmap": [
            "src/SessionManagement/Database/",
            "src/SessionManagement/Http/",
            "src/SessionManagement/Exception/",
            "src/SessionManagement/Auth/",
            "src/UserAuth/Database/",
            "src/UserAuth/Http/",
            "src/UserAuth/Exception/",
            "src/UserAuth/Validation/"
        ]
    }
}